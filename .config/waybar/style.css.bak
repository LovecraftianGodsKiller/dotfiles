* {
    border: none;
    border-radius: 0;
    font-family: JetBrainsMono nerd font;
    font-weight: bold;
    font-size: 14px;
    min-height: 0;
}

window#waybar {
    background: rgba(25, 23, 36, 0);
    color: #c4a7e7;
    margin-right: 10px;
    margin-left: 10px;
    margin-top: 10px;
    margin-bottom: 10px;
}

tooltip {
    background: #191724;
    border-radius: 10px;
    border-width: 2px;
    border-style: solid;
    border-color: #1f1d2e;
}

#workspaces button {
    padding: 5px;
    background: #191724;
    color: #1f1d2e;
    margin-right: 5px;
}

#workspaces button.active {
    color: #c4a7e7;
}

#workspaces button.focused {
    color: #c4a7e7;
    background: #191724;
    border-radius: 10px;
}

#workspaces button.urgent {
    color: #c4a7e7;
    background: #191724;
    border-radius: 10px;
}

#workspaces button:hover {
    background: #191724;
    color: #c4a7e7;
    border-radius: 10px;
}


#window,
#clock,
#battery,
#temperature,
#pulseaudio,
#network,
#cpu,
#memory,
#workspaces,
#tray,
#backlight {
    background: #191724;
    padding: 0px 10px;
    margin: 3px 0px;
    margin-top: 10px;
    border: 1px solid #1f1d2e;
}

#tray {
    border-radius: 8px;
    margin-right: 10px;
}

#workspaces {
    background: #191724;
    border-radius: 8px;
    margin-left: 10px;
    padding-right: 0px;
    padding-left: 5px;
}



#window {
    border-radius: 8px;
    margin-left: 60px;
    margin-right: 60px;
}

#clock {
    color: #c4a7e7;
    border-radius: 8px;
    margin-left: 10px;
    margin-right: 10px;
}
#temperature {
    color: #eb6f92;
    border-radius: 8px 0 0 8px;
    border-right: 0px;
    margin-right: 0px;
}

#memory {
    color: #9ccfd8;
    border-right: 0;
    border-left: 0;
    border-radius: 8px;
}


#network {
  border-left: 0px;
  border-right: 0px;
  border-radius: 8px;
  margin-left: 10px;
}
#pulseaudio {
    color: #c4a7e7;
    border-left: 0px;
    border-right: 0px;
    border-radius: 8px;
}

#battery {
    color: #317f84;
    border-radius: 0 8px 8px 0;
    margin-right: 10px;
    border-left: 0px;
}

